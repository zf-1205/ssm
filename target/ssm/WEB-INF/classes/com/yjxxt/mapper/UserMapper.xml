<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yjxxt.dao.UserDao">
    <select id="queryUserByUserId" parameterType="int" resultType="com.yjxxt.po.User">
select user_id as userId,user_name as userName,user_pwd as userPwd from tb_user where user_id = #{userId}
</select>
</mapper>